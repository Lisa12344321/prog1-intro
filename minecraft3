import random

from random import randint

#100 liv, int
player_life = 100
zombie_life = 100

#Nedräkning
print("1")
print("2")
print("3")
print("fight")



while zombie_life > 0:
    zombie_life -= randint(1,6) #minus liv, random int mellan 1 och 6
    player_life -= randint(1,6) #minus liv, random int mellan 1 och 6

#om spelarens liv är mer än zombiens vinner spelaren, annars vinner zombien
if player_life > zombie_life:
    print("Player vann")
else:
    print("Zombien vann")